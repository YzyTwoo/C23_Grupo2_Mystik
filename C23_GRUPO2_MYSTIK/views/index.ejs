<!-- head -->

<%- include('./partials/head.ejs') %>
<body>

    <header>
        <nav class="nav-general">
            <div>
                <a href="#" class="line">+54 11 12345678 |</a>
                <a href="#" class="line">consultas@mystikindumentaria.com |</a>
                <a href="#">Ayuda</a>
            </div>
            <div>
                <a href="#" class="line">Crear Cuenta | </a>
                <a href="#">Iniciar Sesión</a>
            </div>
  
        </nav>
  
        <div class="header-top">
            
            <div class="navbar-header">
                
                <div class="hamburguesa">
                    
                    <button class="hamburguesabtn"><i class="fas fa-bars"></i></button>
                    <div class="hamburguesa-content">
                        <a href="#"><strong>INICIO</strong></a> 
                        <a href="#"><strong>TIENDA</strong></a> 
                        <a href="#"><strong>COLECCIÓN</strong></a> 
                        <br>
                        <a href="#"><strong><p class="account">Iniciar Sesión </p></strong> <i class="fas fa-sign-out-alt"></i></a> 
                        <a href="#"><strong><p class="account">Registrate</p></strong> <i class="far fa-id-card"></i></a> 
                        <a href="#" class="line-end"><strong>POLITICA DE DEVOLUCIÓN</strong></a>
                    </div>
                </div>
            
            </div>
            
            <div class="logo-container">
                <a href="#"><img src="/images/LOGO/LogoDesktop/logo.svg" class="logo-general" alt=""></a>
                <a href="#"><img src="/images/LOGO/LogoMobile/logo-mobile.svg"  style="width: 35px;" class="logo-mobile" alt="Logo"></a>
            </div>
            
            <div class="search-cart">
                <input type="text" placeholder="Buscar"><i class="fas fa-search"></i>
                <div class="cart">
                    <i class="fa-solid fa-bag-shopping"></i>
                    <p class="precio">$0</p>
                </div>
            </div>
        </div>
  
        <div class="options-desktop">
            <a href="#"><strong>INICIO</strong></a> 
            <a href="#"><strong>TIENDA</strong></a> 
            <a href="#"><strong>COLECCIÓN</strong></a>
            <a href="#" class="line-end"><strong>POLITICA DE DEVOLUCIÓN</strong></a>
        </div>
    
    </header>

    <main class="main-home">

        <!-- CARRUSEL -->
        <div class="carrousel">
        <div id="carouselExampleFade" class="carousel slide carousel-fade">
            <div class="carousel-inner">
              <div class="carousel-item active">
                <img src="/images/banner/banner.jpg" class="d-block w-100" alt="...">
              </div>
              <div class="carousel-item">
                <img src="/images/banner/banner.jpg" class="d-block w-100" alt="...">
              </div>
              <div class="carousel-item">
                <img src="/images/banner/banner.jpg" class="d-block w-100" alt="...">
              </div>
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleFade" data-bs-slide="prev">
              <span class="carousel-control-prev-icon" aria-hidden="true"></span>
              <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleFade" data-bs-slide="next">
              <span class="carousel-control-next-icon" aria-hidden="true"></span>
              <span class="visually-hidden">Next</span>
            </button>
          </div>
        </div>
          <!-- fin de carrusel -->
    
          <!-- SECCION PRODUCTOS -->
    
          <section class="section-home">
    
            <% productos.forEach(function(producto) { %>
                <article class="home-article">
                  <img class="article-img" src="/images/prod_de_arriba/<%= producto.image %>" alt="">
                  <img class="article-img" src="/images/prod_de_abajo/<%= producto.image %>" alt="">
                  <div class="article-description">
                    <h4 class="title-article"><%= producto.name %></h4>
                    <p class="article-price">$<%= producto.price %></p>
                    <p class="article-pay"><%= producto.description %></p>
                    <a href="#">VER COLECCIÓN</a>
                  </div>
                </article>
              <% }); %>
    
          </section>
    
          <!-- fin de sección productos -->
    
          <!-- ASIDE ESPECIAL -->
    
          <aside class="home-aside">
    
            <section class="section-aside">
    
                <div class="img-aside">
                    <a href="#"><img src="/images/prod_de_arriba/productoColeccion.jpg"    alt=""></a> 
                </div>
    
                <div class="description-aside">
                    <h4>SPRING COLLECTION</h4>
                    <p>6 Cuotas sin interés en compras superiores a $60.000</p>
                </div>
    
            </section>
    
            <section class="section-aside">
    
                <div class="img-aside">
                    <a href="#"><img src="/images/prod_de_abajo/collectionDibujo.jpg" alt=""></a>
                </div>
    
                <div class="description-aside">
                    <h4>SPRING COLLECTION</h4>
                    <p>6 Cuotas sin interés en compras superiores a $60.000</p>
                </div>
                
            </section>
          </aside>
    
          <!-- fin aside -->
    
          
    
      </main>
    
      <div class="article-description"><a href="#">Ver mas productos</a></div> <br>

      

<!-- footer -->

<%- include('./partials/footer.ejs') %>



    
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>

  
</body>

</html>
